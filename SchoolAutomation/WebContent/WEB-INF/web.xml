    <servlet-name>sturegister</servlet-name>
    <url-pattern>/studentregister</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>updatefac</servlet-name>
    <servlet-class>com.silicon.crud.UpdateFaculty</servlet-class>
  </servlet>

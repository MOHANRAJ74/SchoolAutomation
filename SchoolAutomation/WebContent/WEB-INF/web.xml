    <servlet-class>com.silicon.crud.LoginControl</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>login</servlet-name>
    <url-pattern>/loginpage</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>facregisterr</servlet-name>
    <servlet-class>com.silicon.crud.FacultyRegister</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>facregisterr</servlet-name>
    <url-pattern>/facultyregister</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>sturegister</servlet-name>

    <servlet-name>insertmark</servlet-name>
    <url-pattern>/markregister</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>updatemark</servlet-name>
    <servlet-class>com.silicon.crud.MarkUpdate</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>updatemark</servlet-name>
    <url-pattern>/markupdate</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>fgtuname</servlet-name>
    <servlet-class>com.silicon.crud.ForgotUsername</servlet-class>
  </servlet>
